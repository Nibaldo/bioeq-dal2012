<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Medicamento" table="medicamento" lazy="false">
		<id name="registro" column="registro" type="string">
			<generator class="native">
			</generator>
		</id>
		<property name="id" column="id" type="integer" length="10" not-null="false" lazy="false"/>
		<property name="principio_activo" column="principio_activo" type="string" length="255" not-null="false" lazy="false"/>
		<property name="producto" column="producto" type="string" length="255" not-null="false" lazy="false"/>
		<property name="resolucion" column="resolucion" type="string" length="255" not-null="false" lazy="false"/>
		<property name="fecha_resolucion" column="fecha_resolucion" type="string" length="255" not-null="false" lazy="false"/>
		<property name="uso_tratamiento" column="uso_tratamiento" type="string" length="255" not-null="false" lazy="false"/>
		<property name="titular" column="titular" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
